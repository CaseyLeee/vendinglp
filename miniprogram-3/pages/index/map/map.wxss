/* pages/index/map/map.wxss */


.wrapper {

  height: 100%;

}

.block {
  width: 100%;
  height: 120px;
  border-radius: 10rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0;
}

.btn {
  position: fixed;
  bottom: 20rpx;
  height: 100rpx;
  background: darkcyan;
  color: white;

  width: 300rpx;
  left: calc(50% - 150rpx);

  border-radius: 20rpx;
  line-height: 100rpx;
  text-align: center;
}